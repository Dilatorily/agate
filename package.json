{
  "name": "agate",
  "version": "0.0.1",
  "description": "Electron-based music player",
  "scripts": {
    "clean": "rimraf dist",
    "build:electron": "webpack --config src/electron/webpack.config.js",
    "build:client": "webpack --config src/client/webpack.config.js",
    "prebuild": "npm run clean",
    "build": "npm run build:electron && npm run build:client",
    "prestart": "npm run build",
    "start": "electron dist",
    "test": "nyc --reporter=lcov -n=src/**/*.{js,jsx} babel-node tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dilatorily/agate.git"
  },
  "keywords": [
    "es2015",
    "babel",
    "electron",
    "webpack",
    "music"
  ],
  "author": "Dilatorily",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dilatorily/agate/issues"
  },
  "homepage": "https://github.com/Dilatorily/agate#readme",
  "dependencies": {
    "electron-prebuilt": "^1.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "jsdom": "^9.6.0",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.2"
  }
}
