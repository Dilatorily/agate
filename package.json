{
  "name": "agate",
  "version": "0.0.1",
  "description": "Electron-based music player",
  "scripts": {
    "clean": "rimraf dist",
    "build:application": "webpack --config src/application/webpack.config.js",
    "build:browser": "webpack --config src/browser/webpack.config.js",
    "prebuild": "npm run clean",
    "build": "concurrently -kr \"npm run build:application\" \"npm run build:browser\"",
    "prestart": "npm run build",
    "start": "electron dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dilatorily/agate.git"
  },
  "keywords": [
    "es2015",
    "babel",
    "electron",
    "webpack",
    "music"
  ],
  "author": "Dilatorily",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dilatorily/agate/issues"
  },
  "homepage": "https://github.com/Dilatorily/agate#readme",
  "dependencies": {
    "electron-prebuilt": "^1.2.7"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "concurrently": "^2.2.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "html-webpack-plugin": "^2.22.0",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.1"
  }
}
